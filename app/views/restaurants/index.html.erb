<div class="container mt-5">
  <h1>Restaurants</h1>
  <ul>
    <% @restaurants.each do |restaurant| %>
    <li><%= link_to restaurant.name, restaurant %> - <%= restaurant.address %> - <%= restaurant.category %> - <%= restaurant.phone_number %> </li>
    <% end %>
  </ul>
  <%= link_to 'Add a restaurant', new_restaurant_path %>
</div>
